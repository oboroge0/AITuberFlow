{
  "id": "template-http-api",
  "name": "HTTP API Example",
  "name_ja": "HTTP APIサンプル",
  "description": "Demonstrates making HTTP requests to external APIs.",
  "description_ja": "外部APIへのHTTPリクエストを実行するサンプル。",
  "nodes": [
    {
      "id": "node-http",
      "type": "http-request",
      "position": { "x": 100, "y": 200 },
      "config": {
        "url": "https://api.github.com/zen",
        "method": "GET",
        "headers": {
          "Accept": "text/plain"
        }
      }
    },
    {
      "id": "node-llm",
      "type": "openai-llm",
      "position": { "x": 350, "y": 200 },
      "config": {
        "apiKey": "",
        "model": "gpt-4o-mini",
        "systemPrompt": "You received a zen quote from GitHub. Comment on it briefly and positively.",
        "temperature": 0.7,
        "maxTokens": 128
      }
    },
    {
      "id": "node-output",
      "type": "console-output",
      "position": { "x": 600, "y": 200 },
      "config": {
        "prefix": "[Wisdom]",
        "logLevel": "info"
      }
    }
  ],
  "connections": [
    {
      "id": "conn-1",
      "from": { "nodeId": "node-http", "port": "response" },
      "to": { "nodeId": "node-llm", "port": "prompt" }
    },
    {
      "id": "conn-2",
      "from": { "nodeId": "node-llm", "port": "response" },
      "to": { "nodeId": "node-output", "port": "text" }
    }
  ],
  "character": {
    "name": "API Bot",
    "personality": "Curious and informative"
  }
}
