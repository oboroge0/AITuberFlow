{
  "id": "template-multi-llm",
  "name": "Multi-LLM Comparison",
  "name_ja": "複数LLM比較",
  "description": "Send the same prompt to multiple LLM providers and compare responses.",
  "description_ja": "同じプロンプトを複数のLLMプロバイダーに送信し、応答を比較。",
  "nodes": [
    {
      "id": "node-input",
      "type": "manual-input",
      "position": { "x": 100, "y": 250 },
      "config": {
        "placeholder": "Enter a question...",
        "inputText": "What is the meaning of life in one sentence?"
      }
    },
    {
      "id": "node-openai",
      "type": "openai-llm",
      "position": { "x": 350, "y": 100 },
      "config": {
        "apiKey": "",
        "model": "gpt-4o-mini",
        "systemPrompt": "Answer concisely in one sentence.",
        "temperature": 0.7,
        "maxTokens": 100
      }
    },
    {
      "id": "node-anthropic",
      "type": "anthropic-llm",
      "position": { "x": 350, "y": 250 },
      "config": {
        "apiKey": "",
        "model": "claude-3-5-haiku-latest",
        "systemPrompt": "Answer concisely in one sentence.",
        "temperature": 0.7,
        "maxTokens": 100
      }
    },
    {
      "id": "node-google",
      "type": "google-llm",
      "position": { "x": 350, "y": 400 },
      "config": {
        "apiKey": "",
        "model": "gemini-2.0-flash",
        "systemPrompt": "Answer concisely in one sentence.",
        "temperature": 0.7,
        "maxTokens": 100
      }
    },
    {
      "id": "node-output-openai",
      "type": "console-output",
      "position": { "x": 600, "y": 100 },
      "config": {
        "prefix": "[OpenAI]",
        "logLevel": "info"
      }
    },
    {
      "id": "node-output-anthropic",
      "type": "console-output",
      "position": { "x": 600, "y": 250 },
      "config": {
        "prefix": "[Claude]",
        "logLevel": "info"
      }
    },
    {
      "id": "node-output-google",
      "type": "console-output",
      "position": { "x": 600, "y": 400 },
      "config": {
        "prefix": "[Gemini]",
        "logLevel": "info"
      }
    }
  ],
  "connections": [
    {
      "id": "conn-1",
      "from": { "nodeId": "node-input", "port": "text" },
      "to": { "nodeId": "node-openai", "port": "prompt" }
    },
    {
      "id": "conn-2",
      "from": { "nodeId": "node-input", "port": "text" },
      "to": { "nodeId": "node-anthropic", "port": "prompt" }
    },
    {
      "id": "conn-3",
      "from": { "nodeId": "node-input", "port": "text" },
      "to": { "nodeId": "node-google", "port": "prompt" }
    },
    {
      "id": "conn-4",
      "from": { "nodeId": "node-openai", "port": "response" },
      "to": { "nodeId": "node-output-openai", "port": "text" }
    },
    {
      "id": "conn-5",
      "from": { "nodeId": "node-anthropic", "port": "response" },
      "to": { "nodeId": "node-output-anthropic", "port": "text" }
    },
    {
      "id": "conn-6",
      "from": { "nodeId": "node-google", "port": "response" },
      "to": { "nodeId": "node-output-google", "port": "text" }
    }
  ],
  "character": {
    "name": "Multi AI",
    "personality": "Diverse perspectives from multiple AI models"
  }
}
